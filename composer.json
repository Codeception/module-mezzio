{
    "name":"codeception/module-zendexpressive",
    "description":"Codeception module for Zend Expressive framework",
    "keywords":["codeception", "expressive", "zend-expressive"],
    "homepage":"http://codeception.com/",
    "type":"library",
    "license":"MIT",
    "authors":[
        {
            "name":"Gintautas Miselis"
        }
    ],
    "minimum-stability": "RC",

    "require": {
        "php": ">=5.6.0 <8.0",
        "codeception/lib-innerbrowser": "dev-master",
        "codeception/codeception": "4.0.x-dev"
    },
    "require-dev": {
        "codeception/util-robohelpers": "dev-master",
        "codeception/module-rest": "dev-master",
        "zendframework/zend-expressive": "~2.0 | ~3.0"
    },
    "autoload":{
        "classmap": ["src/"]
    },
    "config": {
        "classmap-authoritative": true
    }
}
